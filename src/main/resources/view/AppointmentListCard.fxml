<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox id="cardPane" fx:id="cardPane" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="id" styleClass="cell_large_label" text="\$id" textFill="WHITE"></Label>
      <Label fx:id="appointmentDateTime" styleClass="cell_large_label" text="\$appointmentDateTime" textFill="WHITE">
          <graphic>
              <ImageView fitHeight="17.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                      <Image url="@../images/phone.png" />
                  </image>
              </ImageView>
          </graphic>
   </Label>
   <Label fx:id="appointmentLocation" styleClass="cell_large_label" text="\$appointmentLocation" textFill="WHITE">
      <graphic>
         <ImageView fitHeight="17.0" fitWidth="51.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../images/phone.png" />
            </image>
         </ImageView>
      </graphic>
   </Label>
</HBox>
